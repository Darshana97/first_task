<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent" android:background="@drawable/background">

    <ImageView
            android:layout_width="78dp"
            android:layout_height="81dp" app:srcCompat="@mipmap/ic_launcher_round"
            android:id="@+id/imageView4" android:layout_marginTop="76dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.47" android:contentDescription="@string/todo4"/>
    <TextView
            android:text="@string/darshana3"
            android:layout_width="116dp"
            android:layout_height="40dp"
            android:id="@+id/textView" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView4" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.534" android:textStyle="bold"
            android:textColor="#0C0C0C" android:textSize="20sp"/>
    <ImageView
            android:layout_width="411dp"
            android:layout_height="377dp" app:srcCompat="@android:color/holo_blue_light"
            android:id="@+id/imageView5" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:contentDescription="@string/todo5"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="@string/username5"
            android:layout_width="109dp"
            android:layout_height="18dp"
            android:id="@+id/textView5" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.22"
            android:textColor="#F1EEEE" android:layout_marginTop="40dp"
            app:layout_constraintTop_toTopOf="@+id/imageView5"/>
    <EditText
            android:layout_width="235dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText2"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.393"
            app:layout_constraintTop_toBottomOf="@+id/textView5" android:autofillHints=""/>
    <TextView
            android:text="@string/password2"
            android:layout_width="104dp"
            android:layout_height="19dp"
            android:id="@+id/textView6" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/editText2" app:layout_constraintHorizontal_bias="0.226"
            android:textColor="#F8F6F6"/>
    <EditText
            android:layout_width="232dp"
            android:layout_height="44dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editText3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6" app:layout_constraintHorizontal_bias="0.404"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/button2"
            app:layout_constraintVertical_bias="0.0" android:autofillHints=""/>
    <Button
            android:text="@string/login4"
            android:layout_width="227dp"
            android:layout_height="53dp"
            android:id="@+id/button2" android:layout_marginBottom="64dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.44"
            android:background="@drawable/custom_button"
            android:textColor="#F5F1F1" android:backgroundTint="#03A9F4" android:backgroundTintMode="add"/>
</androidx.constraintlayout.widget.ConstraintLayout>